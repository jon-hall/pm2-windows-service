{
	"env": {
		"es6": true,
		"node": true
	},
	"extends": ["airbnb-base", "plugin:jest/recommended", "prettier"],
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"plugins": ["import", "jest", "json"],
	"rules": {
		"import/no-extraneous-dependencies": [
			"error",
			{
				"devDependencies": true
			}
		],
		"lines-between-class-members": "error",
		"no-console": "off",
		"prefer-destructuring": "off"
	}
}
