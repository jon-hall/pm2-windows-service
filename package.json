{
	"name": "@fdawgs/pm2-windows-service",
	"version": "1.0.4",
	"description": "Easily install and uninstall PM2 as a Windows service",
	"keywords": [
		"pm2",
		"windows",
		"service"
	],
	"main": "src/index.js",
	"repository": "git@github.com:Fdawgs/pm2-windows-service.git",
	"author": "Jon Hall <jon_hall@outlook.com>",
	"contributors": [
		"Frazer Smith"
	],
	"license": "MIT",
	"engines": {
		"node": ">=12.16.3"
	},
	"bin": {
		"pm2-service-install": "bin/pm2-service-install",
		"pm2-service-uninstall": "bin/pm2-service-uninstall"
	},
	"scripts": {
		"changelog": "conventional-changelog -p @commitlint/config-conventional -s -i CHANGELOG.md && prettier CHANGELOG.md --write",
		"lint": "eslint . --cache --ext js,jsx,ts,tsx",
		"prettier": "prettier . --write",
		"start": "node bin/pm2-service-install",
		"stop": "node bin/pm2-service-uninstall",
		"test": "node test/integration.js"
	},
	"prettier": {
		"arrowParens": "always",
		"bracketSpacing": true,
		"endOfLine": "lf",
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "none",
		"useTabs": true
	},
	"os": [
		"win32"
	],
	"devDependencies": {
		"@commitlint/config-conventional": "^9.1.1",
		"conventional-changelog-cli": "^2.0.35",
		"eslint": "^7.6.0",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jest": "^23.20.0",
		"eslint-plugin-jsdoc": "^30.2.0",
		"eslint-plugin-json": "^2.1.2",
		"eslint-plugin-promise": "^4.2.1",
		"prettier": "2.0.5",
		"temp": "^0.9.1"
	},
	"dependencies": {
		"co": "^4.6.0",
		"co-event": "^0.1.0",
		"del": "^5.1.0",
		"fs-extra": "^9.0.1",
		"inquirer": "^7.3.3",
		"is-admin": "^3.0.0",
		"node-windows": "1.0.0-beta.4",
		"promisify-node": "^0.5.0",
		"shelljs": "^0.8.4",
		"yargs": "^15.4.1"
	}
}
